@using Oracle.ManagedDataAccess.Client;
@using WebCMSJIR.Views.Frequentation;
@{ 
    FonctionFreq fFreq = new FonctionFreq();
    var codeAct = ViewBag.Message;
    OracleDataReader drPrest = fFreq.ListePrest(codeAct);
}
    <label>Prestation</label>
    <select style="border: 1px solid dodgerblue" class="form-control" name="prest" onchange="ListeMed(this.value)">    
    <option value="#"></option>
    @while (drPrest.Read())
    {
        <option value="@drPrest.GetOracleValue(0)">@drPrest.GetOracleValue(1)</option>
    }
</select>
